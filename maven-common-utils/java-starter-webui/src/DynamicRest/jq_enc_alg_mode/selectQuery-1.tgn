SELECT DISTINCT eam.`mode_id` AS modeId, eam.`mode_name` AS modeName, eap.`padding_id` AS paddingId, eap.`padding_name` AS paddingName, ekl.key_length_id AS keyLengthId, ekl.key_length AS keyLength  FROM `jq_enc_alg_mod_pad_key_lookup` eal,`jq_enc_mode_lookup` eam, `jq_enc_padd_lookup` eap, jq_encryption_algorithms_lookup acd, `jq_enc_key_length_lookup` ekl  WHERE eam.`mode_id` = eal.`enc_mode_id`AND eap.`padding_id`= eal.`enc_padding_id` AND eal.enc_algorithm_id = acd.encryption_algo_id AND ekl.key_length_id = eal.enc_key_length AND eal.enc_algorithm_id =:algorithmId