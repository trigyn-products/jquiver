SELECT js.scheduler_id,js.scheduler_name, js.header_json, js.request_param_json, js.failed_notification_params, jdrd.jws_dynamic_rest_id AS jws_dynamic_rest_id,jdrd.jws_dynamic_rest_url AS rest_api_url,jrtd.jws_request_type AS jws_request_type,cron_scheduler,js.is_active FROM jq_job_scheduler js, jq_dynamic_rest_details jdrd, jq_request_type_details jrtd WHERE jdrd.jws_dynamic_rest_id=js.jws_dynamic_rest_id AND jdrd.jws_request_type_id = jrtd.jws_request_type_details_id AND js.scheduler_id = <#if schedulerid??> :schedulerid </#if>