SELECT cd.client_id AS clientId, cd.client_name, cd.client_key, cd.client_secret, cd.client_public_key, ed.encryption_algo_id AS encryptionAlgoId, cd.updated_by, cd.created_by, cd.updated_date, ed.encryption_algo_name AS encryptionAlgoName, eam.mode_id AS modeId, eam.mode_name AS modeName, eap.padding_id AS paddingId, eap.padding_name AS paddingName, ekl.key_length_id AS keyLengthId FROM jq_api_client_details cd, jq_encryption_algorithms_lookup ed, jq_enc_alg_mod_pad_key_lookup eal, jq_enc_mode_lookup AS eam, jq_enc_padd_lookup AS eap, jq_enc_key_length_lookup AS ekl
WHERE cd.encryption_algo_id = eal.enc_lookup_id AND ed.encryption_algo_id = eal.enc_algorithm_id 
AND eal.enc_mode_id = eam.mode_id AND eal.enc_padding_id = eap.padding_id AND eal.enc_key_length = ekl.key_length_id AND cd.client_id = <#if clientid??> :clientid </#if>