<rest>
    <url></url> <!-- mandatory -->
    <type></type> <!-- mandatory -->
    <content-type></content-type>
    <timeout></timeout>
    <request>
        <headers> <!-- optional-->
            <parameter>												<!-- If authetication is enabled and bearer token is either passed or generated by the application,
                <name>at</name>											 then in header "at" property has to be passed with value as
                <value>7d1oa821 | 7d1dba821 | 7d1ldap821</value>		 7d1oa821 for OAuth, 7d1dba821 for DB Authentication and 7d1ldap821 for LDAP Authentication -->
            </parameter>
            <parameter>
                <name></name>
                <value></value>
            </parameter>
            <parameter>
                <name></name>
                <value></value>
            </parameter>
        </headers>
        <query-param> <!-- optional-->
            <parameter>
                <name></name>
                <value></value>
            </parameter>
            <parameter>
                <name></name>
                <value></value>
            </parameter>
            <parameter>
                <name></name>
                <value></value>
            </parameter>
        </query-param>
        <body contentType="rawBody | keyValue"> <!-- body and contenttype in body are optional-->
        <!-- 
        	null / no mention / keyValue							: pass key value pair in request based on request type 
        	rawBody 												: Will just dump the body in the request, all child elements will be ignored in this scenario. 
        															  This will not support file attachment. Make sure the parameter name is "data-raw" if the contentType is "rawBody"
        -->
            <parameter>
           		<name>data-raw</name>		<!-- data-raw			: if the contentType is rawBody, is mandatory.  -->
                <value></value>
            </parameter>
            <parameter dataType="json">		<!--  json				: will create json object with the key value provided and pass in the body -->
                <name></name>				<!-- user defined key	: user defined key, as per the user request -->
                <value></value>
            </parameter>
            <parameter>			<!-- datatype null / no mention		: pass key value pair in request based on request type -->
           		<name></name>				<!-- user defined key	: user defined key, as per the user request -->
                <value></value>
            </parameter>
        </body> 
         <attachments>
	      <attachment type="1|2|3" hasEmbeddedImage="imageName"> <!-- hasEmbeddedImageis optional -->
	         <!-- 1=internal uploaded file;2=filesystem;3=uploaded file in this call -->
	          <name ></name>
	         <path></path>
	         <!-- if type=1 then fileuploadid;else absolute file path,else the uploaded file -->
	      </attachment>
	   </attachments>
    </request>
</rest>